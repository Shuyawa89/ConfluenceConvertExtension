{
    "manifest_version": 3,
    "name": "Confluence to Markdown",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.atlassian.net/*",
                "*://*/wiki/*"
            ],
            "js": [
                "src/content.ts"
            ]
        }
    ]
}